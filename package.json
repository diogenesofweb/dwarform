{
	"name": "dwarform",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json --compiler-warnings 'a11y-autofocus:ignore'",
		"check:error": "svelte-check --tsconfig ./tsconfig.json --compiler-warnings 'a11y-autofocus:ignore' --threshold error",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.19",
		"@sveltejs/kit": "next",
		"autoprefixer": "^10.3.4",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.42.6",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"tslib": "^2.3.1",
		"typescript": "^4.6.3"
	},
	"type": "module",
	"dependencies": {
		"@kazkadien/svelte": "^0.10.0",
		"prismjs": "^1.25.0"
	}
}
